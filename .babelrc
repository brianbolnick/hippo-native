{
  "presets": ["babel-preset-expo"],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "screens": "./src/screens",
        "components": "./src/components",
        "images": "./src/images",
        "utils": "./src/utils",
				"src": "./src"
      },
      "extensions": [".js"]
    }]
  ]
}
