{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@components": "./src/components",
        "@utils": "./src/utils",
        "@screens": "./src/screens",
        "@images": "./app/images"
      }
    }]
  ]
}
