{
  "expo": {
    "name": "hungry-hippo-recipes",
    "description": "Recipe Management Application",
    "slug": "hungry_hippo",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FE4969"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.brianbolnick.hungryhippo"
    },
    "androidStatusBar": {
      "backgroundColor": "#FE4969"
    },
    "githubUrl": "https://github.com/brianbolnick/hippo_native",
    "android": {
      "package": "com.brianbolnick.hungryhippo"
    }
  }
}
