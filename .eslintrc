{
	"extends": [
		"react-app",
		"airbnb",
		"prettier",
		"prettier/react"
	],
	"rules": {
		"react/jsx-filename-extension": [
			1,
		{
			"extensions": [
				".js",
			".jsx"
			]
		}
		],
		"prettier/prettier": [
			"error",
		{
			"trailingComma": "es5",
			"singleQuote": true,
			"printWidth": 100
		},
    "no-unused-vars": ["error"]
		]
	},
	"plugins": [
		"prettier"
	],
	"import/resolver": {
		"babel-module": {}
	}
}
